  },
  "test_settings": {
    "default": {
      "launch_url": "https://dashboard.syncano.io",
      "selenium_port": 4747,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": false,
        "path": ""
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "platform": "WINDOWS",
        "javascriptEnabled": true,
        "acceptSslCerts": true
      }
    }
  }
